<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LoginSystemPowerCode.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LoginSystemPowerCode"
    Shell.FlyoutBehavior="Disabled"
    Title="LoginSystemPowerCode">

    <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        ContentTemplate="{DataTemplate local:RegisterSystem}"
        Route="RegisterSystem" />

</Shell>
